CScov=function(p,rho){
return(matrix(rho,p,p)+(1-rho)*diag(p))
}
ARcov=function(p,rho){
s=c(1:p)
for(i in 1:p){
s[i]=rho^(i-1)
}
return(toeplitz(s))
}
B=CScov(10,0.5)
A=ARcov(100,0.9)
C=MASS::mvrnorm(n=100,mu=rep(0,10),CScov(10,0.3))
